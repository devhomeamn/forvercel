<link rel="stylesheet" href="/css/styles.css">
<h1>Login</h1>
<form action="/login" method="POST">
  <div>
    <label>Email:</label>
    <input type="email" name="email" required />
  </div>
  <div>
    <label>Password:</label>
    <input type="password" name="password" required />
  </div>
  <button type="submit">Login</button>
</form>
